{
    "name": "Angular Dev Container",
    "dockerComposeFile": "docker-compose.yml",
    "service": "angular",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/zsh"
            },
            "extensions": [
                "Angular.ng-template",
                "johnpapa.angular-essentials",
                "ms-azuretools.vscode-docker"
            ]
        }
    }
}
